        </rule>
        <rule name="blob" stopProcessing="true">
          <match url="([^/]*)/(.+)" />
          <conditions trackAllCaptures="true">
          </conditions>
          <action type="Rewrite" url="/blob/{R:1}/{R:2}" />
        </rule>
        <rule name="account" stopProcessing="true">
